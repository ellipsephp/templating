{
    "name": "ellipse/templating",
    "type": "library",
    "description": "The templating component of the ellipse framework",
    "keywords": [
        "psr-15",
        "template",
        "templating",
        "plates",
        "twig"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/templating",
    "support": {
        "issues": "https://github.com/ellipsephp/templating/issues",
        "source": "https://github.com/ellipsephp/templating"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "http-interop/http-middleware": "^0.4.0",
        "container-interop/service-provider": "^0.3.0",
        "ellipse/contracts-templating": "^1.0",
        "ellipse/container": "^1.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Templating\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
